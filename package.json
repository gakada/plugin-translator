{
  "name": "poi-plugin-translator",
  "version": "1.1.5",
  "description": "Translate package into English.",
  "main": "index.js",
  "scripts": {
    "reset": "git clean -f && git checkout .",
    "compile": "poi-util-transpile --sm --replace",
    "prepack": "node pack.js && npm run compile",
    "postpublish": "npm run reset",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/poooi/plugin-translator.git"
  },
  "author": {
    "name": "KochiyaOcean",
    "link": "https://github.com/kochiyaocean"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/poooi/plugin-translator/issues"
  },
  "homepage": "https://github.com/poooi/plugin-translator#readme",
  "dependencies": {
    "i18next": "^10.4.1"
  },
  "maintainers": [
    {
      "name": "letspoi",
      "email": "poooipoi@126.com"
    }
  ],
  "poiPlugin": {
    "title": "Translator",
    "id": "resources",
    "description": "Translate ships' & equipments' name into English or Korean",
    "icon": "fa/language",
    "i18nDir": "i18n",
    "apiVer": {
      "7.5.99": "0.10.0"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "@babel/preset-stage-0": "^7.0.0-beta.44",
    "@babel/register": "^7.0.0-beta.44",
    "babel-eslint": "^8.0.1",
    "bluebird": "^3.5.1",
    "eslint": "^4.8.0",
    "eslint-config-airbnb-base": "^12.0.2",
    "eslint-plugin-import": "^2.7.0",
    "filenamify": "^2.0.0",
    "fs-extra": "^5.0.0",
    "glob": "^7.1.2",
    "lodash": "^4.17.5",
    "luaparse": "^0.2.1",
    "nodemw": "^0.12.0",
    "poi-util-transpile": "^7.6.2"
  }
}
